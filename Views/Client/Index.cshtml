@model IEnumerable<wdpr_h.Models.Client>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nicknaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LeeftijdsCategorie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Naam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Achternaam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isKindAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OuderAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HulpverlenerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Wachtwoord)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nicknaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LeeftijdsCategorie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Naam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Achternaam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isKindAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OuderAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HulpverlenerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wachtwoord)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.User">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.User">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.User">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
